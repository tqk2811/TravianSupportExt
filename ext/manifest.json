{
    "name": "Travian Support",
    "description": "Travian Support",
    "version": "0.0.1",
    "permissions": [ 
    ],
    "background": {
        "scripts": [ "/scripts/background.js" ],
        "persistent": true
    },
    "manifest_version": 2,
    "content_scripts" : [
        {
            "run_at" : "document_idle",
            "matches": [
                "https://*.travian.com/*"
            ],
            "js": [
                "/js/jquery-3.6.0.min.js",
                "/js/Prototypes.js",
                "/js/Libs.js"
            ]
        }
    ]
}